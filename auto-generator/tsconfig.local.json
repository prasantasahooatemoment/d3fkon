{
  "compilerOptions": {
    "strict": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "noEmitHelpers": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "ES5",
    "plugins": [
      { "transform": "ts-transformer-keys/transformer" }
    ]
  },
  "files": [
    "filewatcher.ts"
  ],
  
  "exclude": [
    "node_modules"
  ]
}
